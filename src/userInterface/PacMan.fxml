<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane fx:id="pane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="userInterface.pacManController">
   <children>
      <MenuBar layoutY="2.0">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
                  <Menu mnemonicParsing="false" text="Load Game">
                    <items>
                      <MenuItem mnemonicParsing="false" onAction="#loadLevel0" text="Level 0" />
                        <MenuItem mnemonicParsing="false" onAction="#loadLevel1" text="Level 1" />
                        <MenuItem mnemonicParsing="false" onAction="#loadLevel2" text="Level 2" />
                    </items>
                  </Menu>
                  <MenuItem mnemonicParsing="false" text="Save Game" />
                  <MenuItem mnemonicParsing="false" text="Exit" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="View">
            <items>
              <MenuItem mnemonicParsing="false" text="Best Scores" />
                  <MenuItem mnemonicParsing="false" text="Game About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <Label layoutX="11.0" layoutY="35.0" prefHeight="25.0" prefWidth="64.0" text="Rebouns:">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <Label layoutX="80.0" layoutY="39.0" />
   </children>
</Pane>
